{
    "name": "rafik/psc-world-webservice",
    "description": "An SOAP implemented client with defined interface",
    "type": "library",
    "authors": [
        {
            "name": "Brandon Antonio Lorenzo",
            "email": "brandolin_a_@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
